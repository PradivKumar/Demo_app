<%provide(:title, "Users")%>
<br><br>
<h1>Users</h1>

<div class="row">
<aside class="col-md-4">
<section>
<%=render 'shared/search_form'%>
</section>
</aside>
<section class="search_result">
<div class="col-md-5">
<%= will_paginate %>
<ol class="users">
  <% @users.each do |user| %>
     <%=render user%>
  <% end %>
</ol>
<%= will_paginate %>
</div>
</section>
</div>